    "knex": "^0.12.1",
    "mysql": "^2.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.14.0",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
